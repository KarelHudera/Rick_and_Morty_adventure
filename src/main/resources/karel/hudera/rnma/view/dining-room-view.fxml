<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="karel.hudera.rnma.presentation.controllers.DiningRoomViewController"
      prefHeight="400.0" prefWidth="600.0">


    <!-- Use a spacer to push buttons towards the bottom -->
    <Region fx:id="spacer" VBox.vgrow="ALWAYS"/>

    <HBox spacing="20.0" alignment="CENTER" style="-fx-padding: 20 0 10 0;">
        <Button fx:id="kitchenButton" text="Go to Kitchen" onAction="#onKitchenButtonClick"/>
        <Button fx:id="livingRoomButton" text="Go to Living Room" onAction="#onLivingRoomButtonClick"/>
    </HBox>
</VBox>