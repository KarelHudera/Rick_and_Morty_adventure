<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="karel.hudera.rnma.presentation.controllers.DiningRoomViewController">

    <!-- Container for navigation buttons -->
    <HBox spacing="20.0">
        <!-- Button to navigate to the Kitchen -->
        <Button text="Go to Kitchen" onAction="#onKitchenButtonClick"/>
        <!-- Button to navigate to the Living Room -->
        <Button text="Go to Living Room" onAction="#onLivingRoomButtonClick"/>
    </HBox>
</HBox>